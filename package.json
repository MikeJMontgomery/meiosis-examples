{
  "name": "meiosis-examples",
  "version": "0.7.4",
  "description": "meiosis-examples",
  "main": "index.js",
  "scripts": {
    "persistence-library": "(cd persistence/library && npm i && npm start)",
    "library": "npm run persistence-library && (cd examples/library && npm i && npm link ../../persistence/library && npm start)",
    "labeled-sliders": "(cd examples/labeled-sliders && npm i && npm start)",
    "temperatures": "(cd examples/temperatures && npm i && npm start)",
    "todo-list": "(cd examples/todo-list && npm i && npm start)",
    "todomvc": "(cd examples/todomvc && npm i && npm start)",
    "preexamples": "swigtohtml",
    "examples": "npm run labeled-sliders && npm run library && npm run temperatures && npm run todo-list && npm run todomvc",
    "prestart": "npm i && npm run examples",
    "start": "node --harmony server/todo-list/main",
    "prelibrary-server": "npm run library && npm i && (cd server/library && tsc) && npm link persistence/library",
    "library-server": "node server/library/build/main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis-examples.git"
  },
  "keywords": [
    "example",
    "meiosis",
    "reactive"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis-examples/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis-examples#readme",
  "dependencies": {
    "co-body": "^4.2.0",
    "hapi": "^15.1.1",
    "inert": "^4.0.2",
    "koa": "^1.2.4",
    "koa-route": "^2.4.2",
    "koa-static": "^2.0.0",
    "sql.js": "^0.3.2"
  },
  "devDependencies": {
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.1",
    "source-map-loader": "^0.1.5",
    "swig": "^1.4.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
