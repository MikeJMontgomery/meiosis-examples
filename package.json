{
  "name": "meiosis-examples",
  "version": "0.6.0",
  "description": "meiosis-examples",
  "main": "index.js",
  "scripts": {
    "labeled-sliders": "(cd examples/labeled-sliders && npm i && npm start)",
    "todo-list": "(cd examples/todo-list && npm i && npm start)",
    "todomvc": "(cd examples/todomvc && npm i && npm start)",
    "preexamples": "for F in $(find . -name 'index*.swig' | grep -v common) ; do node_modules/swig/bin/swig.js render $F > $(echo $F | sed -e 's/swig/html/'); done",
    "examples": "npm run labeled-sliders && npm run todo-list && npm run todomvc",
    "prestart": "npm i && npm run examples",
    "start": "node --harmony server/main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis-examples.git"
  },
  "keywords": [
    "example",
    "meiosis",
    "reactive"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis-examples/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis-examples#readme",
  "dependencies": {
    "co-body": "^4.2.0",
    "koa": "^1.2.0",
    "koa-route": "^2.4.2",
    "koa-static": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "swig": "^1.4.2"
  }
}
