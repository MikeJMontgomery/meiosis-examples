{
  "name": "realworld-meiosis",
  "version": "1.0.0",
  "description": "meiosis-examples realworld",
  "main": "index.js",
  "scripts": {
    "start": "browserify src/index.js -t [ babelify ] -o build/generated-app.js",
    "watch": "watchify src/index.js -t [ babelify ] -o build/generated-app.js -v",
    "wright": "wright build",
    "reload": "reload -b -w build -p ${PORT:-3000}",
    "server": "static-server -p ${PORT:-3000}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis-examples.git"
  },
  "keywords": [
    "example",
    "meiosis",
    "reactive",
    "realworld",
    "seview"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis-examples/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis-examples#readme",
  "dependencies": {
    "marked": "^0.5.0",
    "mithril": "^1.1.6",
    "mithril-stream": "^1.1.0",
    "patchinko": "^4.1.0",
    "seview": "^0.10.0",
    "tinyfunk": "^1.2.1",
    "url-mapper": "^2.0.0",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "eslint": "^5.5.0",
    "meiosis-tracer": "^2.0.2",
    "reload": "^2.3.1",
    "static-server": "^2.2.1",
    "watchify": "^3.11.0",
    "wright": "^1.2.4"
  }
